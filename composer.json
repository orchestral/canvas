{
    "name": "orchestra/canvas",
    "description": "Code Generators for Laravel Applications and Packages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "bin": [
        "canvas"
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Canvas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Canvas\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/database": "^8.83.27",
        "illuminate/support": "^8.83.27",
        "orchestra/canvas-core": "^6.6",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^1.0.4",
        "orchestra/testbench": "^6.32"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        },
        "laravel": {
            "providers": [
                "Orchestra\\Canvas\\LaravelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
